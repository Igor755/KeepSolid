<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sonikpalms.intern.com.sonikpalms.fragment.FragmentReject">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/reject2"
        android:layout_marginTop="48dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />
    <TextView
        android:id="@+id/textViewReject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User declined the address"
        android:textSize="10sp"
        android:layout_marginTop="46dp"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true" />
    <Button
        android:id="@+id/buttonReject"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:text="OK"
        android:layout_below="@+id/textViewReject"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp" />




</RelativeLayout>
